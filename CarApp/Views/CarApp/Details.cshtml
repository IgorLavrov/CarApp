@model CarApp.Models.CarApp.CarAppDetailsViewModel

<h1>Details</h1>

<div>
    <h4>Car</h4>
    <hr />
    <dl class="row">
        <div class="form-group row">
            <label asp-for="Id" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Id)
        </div>

        <div class="form-group row">
            <label asp-for="Owner" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Owner)
        </div>

        <div class="form-group row">
            <label asp-for="RegistrationNumber" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.RegistrationNumber)
        </div>

        <div class="form-group row">
            <label asp-for="VinNumber" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.VinNumber)
        </div>

        <div class="form-group row">
            <label asp-for="Brand" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Brand)
        </div>

        <div class="form-group row">
            <label asp-for="Model" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Model)
        </div>
        <div class="form-group row">
            <label asp-for="Color" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Color)
        </div>
        <div class="form-group row">
            <label asp-for="TypeOfFuel" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.TypeOfFuel)
        </div>

        <div class="form-group row">
            <label asp-for="EngineCapacity" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.EngineCapacity)
        </div>

        <div class="form-group row">
            <label asp-for="NumberOfCarDoors" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.NumberOfCarDoors)
        </div>
        <div class="form-group row">
            <label asp-for="CarWeight" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.CarWeight)
        </div>


        <div class="form-group row">
            <label asp-for="BuiltAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.BuiltAt)
        </div>

        <div class="form-group row">
            <label asp-for="RegistratedAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.RegistratedAt)
        </div>


    </dl>
    <a asp-action="Index" asp-controller="Car" class="btn btn-danger">Index</a>
</div>


